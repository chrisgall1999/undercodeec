{
  "parser": "@babel/eslint-parser",
  "parserOptions": {
    "requireConfigFile": false,
    "babelOptions": {
      "presets": ["@babel/preset-react"]
    }
  },
  "extends": "react-app",
  "rules": {
    "react/style-prop-object": "off",
    "jsx-a11y/anchor-is-valid": "off",
    "jsx-a11y/heading-has-content": "off",
    "jsx-a11y/iframe-has-title": "off"
  }
}
